<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rental.shinhan">

    <select id="selectOrders" parameterType="int" resultType="SubscribeVO">
        select *
        from subscribe
        where cust_seq = #{cust_seq}
    </select>

</mapper>